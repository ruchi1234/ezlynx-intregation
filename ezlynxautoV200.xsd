            <xsd:enumeration value="12.5/25" />
            <!-- 12.5/25 shown in OH only -->
            <xsd:enumeration value="15/30" />
            <xsd:enumeration value="20/40" />
            <!-- 20/40 shown in CT,IL,IA,MA,MD,MI,WV only -->
            <xsd:enumeration value="20/50" />
            <!-- 20/50 shown in MA,MS only-->
            <xsd:enumeration value="25/25" />
            <xsd:enumeration value="25/50" />
            <xsd:enumeration value="25/60" />
            <!-- 25/60 shown in MA only-->
            <xsd:enumeration value="25/65" />
            <!-- 25/65 shown in UT only -->
            <xsd:enumeration value="30/60" />
            <!-- 30/60 shown in MN,NC,CA only -->
            <xsd:enumeration value="35/80" />
            <!-- 35/80 shown in MA only-->
            <xsd:enumeration value="50/50" />
            <xsd:enumeration value="50/100" />
            <xsd:enumeration value="100/100" />
            <xsd:enumeration value="100/200" />
            <!-- 100/200 shown in MA only-->
            <xsd:enumeration value="100/300" />
            <xsd:enumeration value="200/400" />
            <!-- 200/400 shown in MA only-->
            <xsd:enumeration value="200/600" />
            <!-- 200/600 shown in CT only -->
            <xsd:enumeration value="250/500" />
            <xsd:enumeration value="250/1000" />
            <!-- 250/1000 shown in MA only-->
            <xsd:enumeration value="300/300" />
            <xsd:enumeration value="300/500" />
            <!-- 300/500 shown in MA only-->
            <xsd:enumeration value="500/500" />
            <xsd:enumeration value="500/1000" />
            <!-- 500/1000 Shown in CT,MA only -->
            <xsd:enumeration value="1000/1000" />
            <xsd:enumeration value="55CSL" />
            <xsd:enumeration value="100CSL" />
            <xsd:enumeration value="115CSL" />
            <!-- 115 CSL only shown in WI. Effective Jan 1, 2010 -->
            <xsd:enumeration value="300CSL" />
            <xsd:enumeration value="500CSL" />
            <xsd:enumeration value="1000CSL" />
            <!-- 1000CSL Shown in IN only -->
            <xsd:enumeration value="None" />
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="PolicyTermType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="6 Month" />
            <xsd:enumeration value="12 Month" />
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ReasonForLapseType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Active Military Duty" />
            <xsd:enumeration value="Driving A Company Car" />
            <xsd:enumeration value="Driving A Car Owned By A Relative" />
            <xsd:enumeration value="Has Not Owned Or Operated A Vehicle" />
            <xsd:enumeration value="Owned Veh Parked/Stored Off Road/Not Driven" />
            <xsd:enumeration value="Other" />
            <xsd:enumeration value="Non-Payment" />
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ReasonNoPriorType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Active Military Duty" />
            <xsd:enumeration value="Driving A Company Car" />
            <xsd:enumeration value="Driving A Car Owned By A Relative" />
            <xsd:enumeration value="Has Not Owned Or Operated A Vehicle" />
            <xsd:enumeration value="Owned Veh Parked/Stored Off Road/Not Driven" />
            <xsd:enumeration value="Other" />
            <xsd:enumeration value="Non-Payment" />
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="PriorPolicyInfoType">
        <xsd:sequence>
            <xsd:element name="PriorCarrier" type="PriorCarrierType" />
            <xsd:element name="Expiration" type="DateType" />
            <xsd:element name="YearsWithPriorCarrier" type="DurationType" />
            <xsd:element name="YearsWithContinuousCoverage" type="DurationType" />
            <xsd:element name="PriorLiabilityLimit" type="PriorLiabilityLimitType" />
            <xsd:element name="ReasonForLapse" type="ReasonForLapseType" minOccurs="0" maxOccurs="1" />
            <!-- required when lapse in coverage -->
            <xsd:element name="ReasonNoPrior" type="ReasonNoPriorType" minOccurs="0" maxOccurs="1" />
            <!-- required when No Prior Ins -->
            <xsd:element name="PriorPolicyPremium" type="xsd:nonNegativeInteger" minOccurs="0" maxOccurs="1" />
            <xsd:element name="PriorPolicyTerm" type="PolicyTermType" minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
    </xsd:complexType>

